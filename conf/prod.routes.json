{
    "error": {
        "404": "/themes/flatize/404.php"
    },
    "hosts": [
        "www.pickyshop.com",
        "pickyshop.com",
        "debug.pickyshop.clo.ud.it",
        "dev.pickyshop.com",
        "stage.pickyshop.com",
        "localhost"
    ],
    "routes": [
        {
            "pattern": "/:loc/blog(/tag/:tag/:postTagId)?(/category/:category/:postCategoryId)?(/author/:author/:authorId)?(/)?",
            "behaviour": {
                "ctrl": "\\bamboo\\blog\\controllers\\CBlogHomepageController"
            },
            "filters": {
                "loc": "[a-z]{2}",
                "tag": "[a-zA-Z0-9-]+",
                "postTagId": "[0-9]+",
                "category": "[a-zA-Z0-9-]+",
                "postCategoryId": "[0-9]+",
                "author": "[a-zA-Z0-9-]+",
                "authorId": "[0-9]+"

            },
            "translations": {},
            "methods": [
                "GET"
            ]
        },
        {
            "pattern": "/:loc/blog/:postTitle/:postId",
            "behaviour": {
                "ctrl": "\\bamboo\\blog\\controllers\\CBlogSingleController"
            },
            "filters": {
                "loc": "[a-z]{2}",
                "postId": "[0-9]+",
                "postTitle": "[a-z0-9-]+"
            },
            "translations": {},
            "methods": [
                "GET"
            ]
        }
    ]
}