{
    "error": {
        "404": "/themes/flatize/404.php"
    },
    "hosts": [
        "www.pickyshop.com",
        "pickyshop.com",
        "debug.pickyshop.clo.ud.it",
        "dev.pickyshop.com",
        "stage.pickyshop.com",
        "localhost"
    ],
    "routes": [
        {
            "pattern": "/:loc/blog(/)?",
            "behaviour": {
                "ctrl": "\\bamboo\\blog\\controllers\\CBlogHomepageController"
            },
            "filters": {
                "loc": "[a-z]{2}"
            },
            "translations": {},
            "methods": [
                "GET"
            ]
        },
        {
            "pattern": "/:loc/blog/:postTitle/:postId",
            "behaviour": {
                "ctrl": "\\bamboo\\blog\\controllers\\CBlogSingleController"
            },
            "filters": {
                "loc": "[a-z]{2}",
                "postTitle": "[a-zA-Z0-9-_]{255}",
                "postId": "[0-9]+"
            },
            "translations": {},
            "methods": [
                "GET"
            ]
        }
    ]
}